/dts-v1/;

#include "lan969x_a0.dtsi"
#include "lan969x.dtsi"

/ {
	model = "lan969x UNG8422 EVB";
	config {
		u-boot,mmc-env-partition = "Env";
	};
};

&uart0 {
	status = "okay";
	pinctrl-0 = <&fc0_pins>;
	pinctrl-names = "default";
};

&sdhci0 {
	status = "okay";
};

&emmc {
	status = "okay";
	pinctrl-0 = <&emmc_sd_pins>;
	pinctrl-names = "default";
};

&qspi {
	status = "okay";
	spi-flash@0 {
		status = "okay";
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <104000000>;
		u-boot,dm-pre-reloc;
	};
};
